<%- include('partials/admin-header', { title: 'Game Rooms Management' }) %>

<div class="page-game-rooms">
  <div class="admin-layout">
    <%- include('./partials/admin-sidebar', { currentPage: currentPage || 'game-rooms' }) %>
    <main class="admin-main">
      <%- include('./partials/admin-topbar', { 
        breadcrumb: 'Game Rooms',
        breadcrumbSubtitle: 'Manage multiplayer quiz rooms',
        showSearch: false
    }) %>
      <div class="admin-content">
        <div class="admin-container">
          <div class="admin-header">
            <div class="admin-header-content">
              <h1 class="admin-title">
                <i class="fas fa-gamepad"></i>
                Game Rooms Management
              </h1>
              <p class="admin-subtitle">Manage multiplayer quiz game rooms</p>
            </div>
            <div class="admin-header-actions">
              <a href="/admin/game-rooms/create" class="btn btn-primary">
                <i class="fas fa-plus"></i>
                Create New Room
              </a>
            </div>
          </div>
        </div>


        <!-- Filters Section -->
        <div class="admin-filters">
          <form method="GET" class="filters-form">
            <div class="filter-group">
              <input type="text" name="search" placeholder="Search rooms..." value="<%= filters.search || '' %>" class="form-control">
            </div>
            <div class="filter-group">
              <select name="difficulty" class="form-control">
                <option value="">All Difficulties</option>
                <option value="easy" <%= filters.difficulty === 'easy' ? 'selected' : '' %>>Easy</option>
                <option value="medium" <%= filters.difficulty === 'medium' ? 'selected' : '' %>>Medium</option>
                <option value="hard" <%= filters.difficulty === 'hard' ? 'selected' : '' %>>Hard</option>
              </select>
            </div>
            <div class="filter-group">
              <select name="gameState" class="form-control">
                <option value="">All States</option>
                <option value="waiting" <%= filters.gameState === 'waiting' ? 'selected' : '' %>>Waiting</option>
                <option value="starting" <%= filters.gameState === 'starting' ? 'selected' : '' %>>Starting</option>
                <option value="playing" <%= filters.gameState === 'playing' ? 'selected' : '' %>>Playing</option>
                <option value="finished" <%= filters.gameState === 'finished' ? 'selected' : '' %>>Finished</option>
              </select>
            </div>
            <div class="filter-actions">
              <button type="submit" class="btn btn-secondary">
                <i class="fas fa-search"></i>
                Filter
              </button>
              <a href="/admin/game-rooms" class="btn btn-outline-secondary">
                <i class="fas fa-times"></i>
                Clear
              </a>
            </div>
          </form>
        </div>

        <!-- Game Rooms Grid -->
        <div class="game-rooms-grid">
          <% if (gameRooms.length > 0) { %>
          <% gameRooms.forEach(room => { %>
          <div class="game-room-card">
            <div class="game-room-header">
              <div class="game-room-title">
                <h3><%= room.title %></h3>
                <span class="room-code">#<%= room.roomCode %></span>
              </div>
              <div class="game-room-status">
                <span class="status-badge status-<%= room.gameState %>">
                  <%= room.gameState.charAt(0).toUpperCase() + room.gameState.slice(1) %>
                </span>
              </div>
            </div>

            <div class="game-room-content">
              <div class="game-room-info">
                <% if (room.category) { %>
                <div class="info-item">
                  <i class="fas fa-tag"></i>
                  <span><%= room.category %></span>
                </div>
                <% } %>
                <div class="info-item">
                  <i class="fas fa-signal"></i>
                  <span><%= room.difficulty.charAt(0).toUpperCase() + room.difficulty.slice(1) %></span>
                </div>
                <div class="info-item">
                  <i class="fas fa-users"></i>
                  <span><%= room.currentPlayers.length %>/<%= room.maxPlayers %> players</span>
                </div>
                <div class="info-item">
                  <i class="fas fa-question-circle"></i>
                  <span><%= room.questions.length %> questions</span>
                </div>
                <div class="info-item">
                  <i class="fas fa-clock"></i>
                  <span><%= room.totalTime %> min total</span>
                </div>
              </div>

              <% if (room.description) { %>
              <div class="game-room-description">
                <p><%= room.description %></p>
              </div>
              <% } %>

              <!-- Current Players -->
              <% if (room.currentPlayers.length > 0) { %>
              <div class="current-players">
                <h4>Current Players:</h4>
                <div class="players-list">
                  <% room.currentPlayers.forEach(player => { %>
                  <div class="player-item">
                    <div class="player-avatar">
                      <% if (player.user.profilePicture) { %>
                      <img src="<%= player.user.profilePicture %>" alt="<%= player.user.username %>">
                      <% } else { %>
                      <div class="avatar-fallback">
                        <%= player.user.username.charAt(0).toUpperCase() %>
                      </div>
                      <% } %>
                    </div>
                    <span class="player-name"><%= player.user.username %></span>
                    <% if (player.isReady) { %>
                    <span class="ready-badge">Ready</span>
                    <% } %>
                  </div>
                  <% }); %>
                </div>
              </div>
              <% } %>

              <!-- Winner Display -->
              <% if (room.winner && room.winner.user) { %>
              <div class="winner-display">
                <h4>üèÜ Winner</h4>
                <div class="winner-info">
                  <span class="winner-name"><%= room.winner.user.username || 'N/A' %></span>
                  <span class="winner-score">Score: <%= room.winner.score %></span>
                </div>
              </div>
              <% } %>
            </div>

            <div class="game-room-actions">
              <div class="action-buttons">
                <a href="/admin/game-rooms/<%= room._id %>/stats" class="btn btn-info btn-sm" title="View Statistics">
                  <i class="fas fa-chart-bar"></i>
                  Stats
                </a>
                <a href="/admin/game-rooms/<%= room._id %>/edit" class="btn btn-warning btn-sm" title="Edit Room">
                  <i class="fas fa-edit"></i>
                  Edit
                </a>
                <% if (room.gameState === 'waiting' || room.gameState === 'starting') { %>
                <button class="btn btn-success btn-sm" onclick="adminForceStart('<%= room.roomCode %>')" title="Force Start Game">
                  <i class="fas fa-play"></i>
                  Start
                </button>
                <% } %>
                <% if (room.gameState === 'waiting' || room.gameState === 'finished') { %>
                <button class="btn btn-secondary btn-sm" onclick="deleteRoom('<%= room._id %>')" title="Reset Room">
                  <i class="fas fa-refresh"></i>
                  Reset
                </button>
                <button class="btn btn-danger btn-sm" onclick="confirmDeleteRoom('<%= room._id %>')" title="Delete Room Permanently">
                  <i class="fas fa-trash"></i>
                  Delete
                </button>
                <% } else { %>
                <button class="btn btn-outline-secondary btn-sm" disabled title="Game is Active">
                  <i class="fas fa-lock"></i>
                  Active
                </button>
                <% } %>
              </div>
            </div>
          </div>
          <% }); %>
          <% } else { %>
          <div class="empty-state">
            <div class="empty-state-icon">
              <i class="fas fa-gamepad"></i>
            </div>
            <h3>No Game Rooms Found</h3>
            <p>Create your first game room to get started with multiplayer quizzes.</p>
            <a href="/admin/game-rooms/create" class="btn btn-primary">
              <i class="fas fa-plus"></i>
              Create Game Room
            </a>
          </div>
          <% } %>
        </div>

        <script src="/socket.io/socket.io.js"></script>
        <script>
          const adminSocket = io();

          function adminForceStart(roomCode) {
            if (!confirm('Force start this room now? This will start the game even if the room is not full.')) return;
            adminSocket.emit('admin-force-start', {
              roomCode
            });
            // Give feedback
            alert('Force start requested. Players will be notified.');
          }
        </script>

        <!-- Pagination -->
        <% if (pagination.pages > 1) { %>
        <div class="pagination-wrapper">
          <nav class="pagination">
            <% if (pagination.page > 1) { %>
            <a href="?page=<%= pagination.page - 1 %>" class="pagination-btn">
              <i class="fas fa-chevron-left"></i>
            </a>
            <% } %>

            <% for (let i = 1; i <= pagination.pages; i++) { %>
            <% if (i === pagination.page) { %>
            <span class="pagination-btn active"><%= i %></span>
            <% } else { %>
            <a href="?page=<%= i %>" class="pagination-btn"><%= i %></a>
            <% } %>
            <% } %>

            <% if (pagination.page < pagination.pages) { %>
            <a href="?page=<%= pagination.page + 1 %>" class="pagination-btn">
              <i class="fas fa-chevron-right"></i>
            </a>
            <% } %>
          </nav>
        </div>
        <% } %>
      </div>

      <!-- Reset Confirmation Modal -->
      <div class="modal" id="deleteModal">
        <div class="modal-content">
          <div class="modal-header">
            <h3>Confirm Reset</h3>
            <button class="modal-close" onclick="closeDeleteModal()">
              <i class="fas fa-times"></i>
            </button>
          </div>
          <div class="modal-body">
            <p>Are you sure you want to reset this game room? This will clear all completed sessions and reset the room for new games. This action cannot be undone.</p>
          </div>
          <div class="modal-footer">
            <button class="btn btn-secondary" onclick="closeDeleteModal()">Cancel</button>
            <button class="btn btn-warning" onclick="confirmDelete()">Reset Room</button>
          </div>
        </div>
      </div>

      <!-- Permanent Delete Confirmation Modal -->
      <div class="modal" id="permanentDeleteModal">
        <div class="modal-content">
          <div class="modal-header">
            <h3>Confirm Permanent Deletion</h3>
            <button class="modal-close" onclick="closePermanentDeleteModal()">
              <i class="fas fa-times"></i>
            </button>
          </div>
          <div class="modal-body">
            <div class="alert alert-danger">
              <i class="fas fa-exclamation-triangle me-2"></i>
              <strong>Warning!</strong> This action cannot be undone.
            </div>
            <p>Are you sure you want to permanently delete this game room? This will:</p>
            <ul>
              <li>Delete the game room permanently</li>
              <li>Remove all associated game sessions</li>
              <li>Clear all player data for this room</li>
              <li>Cannot be recovered</li>
            </ul>
          </div>
          <div class="modal-footer">
            <button class="btn btn-secondary" onclick="closePermanentDeleteModal()">Cancel</button>
            <button class="btn btn-danger" onclick="confirmPermanentDelete()">
              <i class="fas fa-trash me-2"></i>Delete Permanently
            </button>
          </div>
        </div>
      </div>

      <script>
        let roomToDelete = null;
        let roomToPermanentlyDelete = null;

        // Reset Room Functions
        function deleteRoom(roomId) {
          roomToDelete = roomId;
          document.getElementById('deleteModal').style.display = 'block';
        }

        function closeDeleteModal() {
          roomToDelete = null;
          document.getElementById('deleteModal').style.display = 'none';
        }

        function confirmDelete() {
          if (roomToDelete) {
            // Simple navigation to GET delete route for reset
            window.location.href = `/admin/game-rooms/${roomToDelete}/delete`;
          }
        }

        // Permanent Delete Room Functions
        function confirmDeleteRoom(roomId) {
          roomToPermanentlyDelete = roomId;
          document.getElementById('permanentDeleteModal').style.display = 'block';
        }

        function closePermanentDeleteModal() {
          roomToPermanentlyDelete = null;
          document.getElementById('permanentDeleteModal').style.display = 'none';
        }

        function confirmPermanentDelete() {
          if (roomToPermanentlyDelete) {
            // Create a form to submit DELETE request for permanent deletion
            const form = document.createElement('form');
            form.method = 'POST';
            form.action = `/admin/game-rooms/${roomToPermanentlyDelete}/permanent-delete`;

            // Add method override for DELETE
            const methodInput = document.createElement('input');
            methodInput.type = 'hidden';
            methodInput.name = '_method';
            methodInput.value = 'DELETE';
            form.appendChild(methodInput);

            document.body.appendChild(form);
            form.submit();
          }
        }

        // Close modal when clicking outside
        window.onclick = function(event) {
          const deleteModal = document.getElementById('deleteModal');
          const permanentDeleteModal = document.getElementById('permanentDeleteModal');

          if (event.target === deleteModal) {
            closeDeleteModal();
          }
          if (event.target === permanentDeleteModal) {
            closePermanentDeleteModal();
          }
        }
      </script>

